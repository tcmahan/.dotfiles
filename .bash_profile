[[ -s "$HOME/.rvm/scripts/rvm" ]] && source "$HOME/.rvm/scripts/rvm"
alias emacs4='/usr/local/Cellar/emacs/HEAD/bin/emacs-24.0.91 -nw'
alias unhide='defaults write com.apple.finder AppleShowAllFiles TRUE ; killall Finder'
alias rehide='defaults write com.apple.finder AppleShowAllFiles FALSE ; killall Finder'
alias la='ls -la'
